# [...] ellipsis

[include]
	path = config-local
[url "https://github.com/xtfxme/"]
	insteadOf = self://

# git directives
[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
[remote "origin"]
	url = self://...
	fetch = refs/heads/*:refs/remotes/origin/*
[branch "ellipsis"]
	remote = origin
	merge = refs/heads/ellipsis
[receive]
	denyCurrentBranch = ignore
[http]
	sslVerify = false

# ellipsis defines
[alias "ellipsis"]
	init = !${GIT_DIR}/self/bin/ellipsis init
