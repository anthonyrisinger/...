[diff]
	noprefix = true
	algorithm = histogram
[stash]
	showPatch = true
[color "diff"]
	meta = yellow
	frag = magenta bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
[core]
	sshCommand = "bash -c 'IFS=; id=~/.ssh/$(git config user.email); [[ -e $id ]] && set -- -i $id $@; exec ssh $@' bash"
	pager = "LESSOPEN=\"||-$(command -v diff-so-fancy) # %s\" && export LESSOPEN || unset LESSOPEN; exec less -x2"
	untrackedCache = true
[commit]
	verbose = true
[format]
	pretty = fuller
